{
  "manifest_version": 3,
  "name": "Whatscraper",
  "description": "Scraps contacts data from web whatsapp",
  "version": "0.0.1",
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "https://web.whatsapp.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/*"],
      "run_at": "document_idle",
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["/*"],
      "matches": ["https://web.whatsapp.com/*"]
    }
  ]
}